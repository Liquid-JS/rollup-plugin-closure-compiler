{
    "name": "@liquid-js/rollup-plugin-closure-compiler",
    "version": "0.27.5",
    "description": "Rollup + Google Closure Compiler",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Liquid-JS/rollup-plugin-closure-compiler.git"
    },
    "author": "The AMP HTML Authors",
    "keywords": [
        "rollup-plugin"
    ],
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "postinstall": "husky",
        "test": "ava",
        "coverage": "c8 ava && c8 report --reporter=html",
        "build": "rimraf dist && rollup -c",
        "release": "git diff --exit-code && standard-version",
        "prepublishOnly": "yarn build",
        "prepack": "clean-package",
        "postpack": "clean-package restore"
    },
    "peerDependencies": {
        "google-closure-compiler": "^20240317.0.0",
        "rollup": ">=4.28"
    },
    "dependencies": {
        "@ampproject/remapping": "^2.3.0",
        "acorn": "^8.14.0",
        "acorn-walk": "^8.3.4",
        "estree-walker": "^3.0.3",
        "magic-string": "^0.30.17"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@rollup/plugin-typescript": "^12.1.2",
        "@stylistic/eslint-plugin": "^2.12.1",
        "@types/estree": "^1.0.6",
        "@types/google-closure-compiler": "^20231112.0.0",
        "@types/node": "<23.0.0",
        "ava": "^6.2.0",
        "builtins": "^5.1.0",
        "c8": "^10.1.3",
        "clean-package": "^2.2.0",
        "codecov": "^3.8.3",
        "eslint": "^9.17.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.6.1",
        "eslint-plugin-prefer-arrow-functions": "^3.4.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "google-closure-compiler": "20210808.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.11",
        "npm-check-updates": "^17.1.11",
        "npm-run-all": "^4.1.5",
        "rimraf": "^6.0.1",
        "rollup": "^4.28.1",
        "standard-version": "^9.5.0",
        "ts-node-maintained": "^10.9.4",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.0"
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix"
        ],
        "*.js": [
            "eslint --fix"
        ],
        "*.cjs": [
            "eslint --fix"
        ]
    },
    "publishConfig": {
        "access": "public"
    },
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        }
    },
    "packageManager": "yarn@4.5.3",
    "clean-package": {
        "remove": [
            "packageManager",
            "scripts",
            "devDependencies",
            "clean-package",
            "lint-staged",
            "publishConfig"
        ]
    }
}
